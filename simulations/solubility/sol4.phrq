
DATABASE ../database/redox.dat

TITLE simulate CO2 partition between aqueous and gas phase
      45 ml N2 gas, 10 ml solution

PHASES
Fix_ph # Define pH fixing mineral
H+ = H+; log_k 0 # pH = 10^(-SI)

SOLUTION 1
  units       mmol/kgw
  temperature 	 4.0
  pH 	      4.0
  water      0.01
  C(4)        10.0 
  Na          10.0 charge
  Cl          10.0 
  Nzero       1.0 redoxN2(g) 0.0
  SAVE SOLUTION 1
END


GAS_PHASE 1 fixed volume gas phase
  -fixed_volume
  -volume        0.045
  temperature 	 4.0
  redoxCO2(g) 0.0
  redoxN2(g)  1.0
  SAVE GAS_PHASE 1
END
 

PRINT
  -reset false
SELECTED_OUTPUT
  -RESET false
  -FILE sol4.txt
USER_PUNCH
  -start
  -heading pH CO2(g) CO2(a) CO2 HCO3- CO3-2
  10 PUNCH -la("H+")
  20 PUNCH GAS('redoxCO2(g)')*1.0e4*100.0 tot('C(4)')*1.0e4 mol('CO2')*1.0e4 mol('HCO3-')*1.0e4 mol('CO3-2')*1.0e4
  -end
END

USE SOLUTION 1; USE GAS_PHASE 1; EQUILIBRIUM_PHASES 1; Fix_ph -4.1 NaOH; END
